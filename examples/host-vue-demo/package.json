{
  "name": "@uniview/example-host-vue",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "run-p bridge plugins:wait vue",
    "bridge": "bun ../host-svelte-demo/server/index.ts",
    "plugins:wait": "sleep 1 && pnpm --filter @uniview/example-plugin client",
    "vue": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@uniview/example-plugin": "workspace:*",
    "@uniview/host-sdk": "workspace:*",
    "@uniview/protocol": "workspace:*",
    "@vueuse/core": "^14.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.563.0",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.4.0",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.18.12",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue-tsc": "^3.1.1"
  }
}
