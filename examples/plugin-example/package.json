{
  "name": "@uniview/example-plugin",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "bun build.ts",
    "dev": "bun build.ts --serve",
    "server": "bun run server:simple & bun run server:advanced",
    "server:simple": "bun src/simple-demo.server.ts",
    "server:advanced": "bun src/advanced-demo.server.ts",
    "client": "bun run client:simple & bun run client:advanced",
    "client:simple": "bun src/simple-demo.client.ts",
    "client:advanced": "bun src/advanced-demo.client.ts",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@uniview/example-plugin-api": "workspace:*",
    "@uniview/runtime": "workspace:*",
    "is-what": "^5.5.0",
    "react": "^18.3.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "@types/react": "^18.3.0",
    "@types/ws": "^8.5.0"
  }
}
