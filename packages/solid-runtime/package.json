{
	"name": "@uniview/solid-runtime",
	"type": "module",
	"version": "0.0.1",
	"description": "Runtime for SolidJS-based Uniview plugins",
	"license": "MIT",
	"exports": {
		".": "./dist/index.mjs",
		"./ws-client": "./dist/ws-client.mjs",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "vitest",
		"check-types": "tsc --noEmit",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "pnpm run build"
	},
	"dependencies": {
		"@uniview/protocol": "workspace:*",
		"@uniview/solid-renderer": "workspace:*",
		"kkrpc": "catalog:"
	},
	"peerDependencies": {
		"solid-js": "^1.9.0"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"@types/ws": "^8.5.0",
		"tsdown": "^0.18.1",
		"typescript": "^5.7.0",
		"vitest": "^2.0.0",
		"ws": "^8.18.0"
	}
}
