{
  "name": "@uniview/host-svelte",
  "type": "module",
  "version": "0.0.1",
  "description": "Svelte 5 host adapter for the Uniview plugin system",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "play": "vite",
    "test": "vitest",
    "check-types": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@uniview/host-sdk": "workspace:*",
    "@uniview/protocol": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^22.0.0",
    "rollup-plugin-svelte": "^7.2.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.3",
    "svelte2tsx": "^0.7.46",
    "tsdown": "^0.18.1",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  }
}
